models/weapons/nailgun_mod_damage
{
	noselfShadow
        unsmoothedTangents

	diffusemap	 downsize( models/weapons/nailgun/nailgun_barrela_d.tga, 1 )
	bumpmap addnormals(models/weapons/nailgun/nailgun_barrela_local.tga, heightmap(models/weapons/nailgun/nailgun_barrela_h.tga, 2 ))
	specularmap	 downsize( models/weapons/nailgun/nailgun_barrela_s.tga, 1 )

	{ 
		if ( parm6 > parm7 && time < parm6 )
		blend add
		map downsize( models/weapons/nailgun/nailgun_barrela_highglow.tga, 1 )
		rgb guitable_random1 [ time * .1 ] * (1 - (.95 * (time-parm7) / (parm6-parm7)))
	}
	{ 
		if ( parm6 >= parm7 && time >= parm6 )
		blend add
		map downsize( models/weapons/nailgun/nailgun_barrela_highglow.tga, 1 )
		rgb guitable_random1 [ time * .1 ] * 0.05
	}
	{ 
		if ( parm7 > parm6 && time < parm7 )
		blend add
		map downsize( models/weapons/nailgun/nailgun_barrela_highglow.tga, 1 )
		rgb guitable_random1 [ time * .1 ] * (0.05 + (.95 * (time-parm6) / (parm7-parm6)))
	}
	{ 
		if ( parm7 > parm6 && time >= parm7 )
		blend add
		map downsize( models/weapons/nailgun/nailgun_barrela_highglow.tga, 1 )
		rgb guitable_random1 [ time * .1 ]
	}
}

